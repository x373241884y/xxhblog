<%
var type = request.query.post_type;
if (!type) {
    type = "list";
} else if (type != "list" && type != "addUI" & type != "category" & type != "tag") {
    type = "list";
}
var breadTip3
if (type == "list") {
    breadTip3 = "所有文章";
}else if(type=="addUI"){
    breadTip3 = "添加文章";
}else if(type=="category") {
    breadTip3 = "分类目录";
}else if(type=="tag") {
    breadTip3 = "标签标签xx";
}
%>
<%- include ../common/bread %>
<!--list-->
<% if(type == "list"){ %>
<%- include list %>
<% } %>
<!--new-->
<% if(type == "addUI"){ %>
<%- include addUI %>
<% } %>
<!--category-->
<% if(type == "category"){ %>
<%- include category %>
<% } %>
<!--tag-->
<% if(type == "tag"){ %>
<%- include tag %>
<% } %>